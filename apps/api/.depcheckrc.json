{
  "ignorePatterns": [
    "*.test.ts",
    "*.test.tsx",
    "*.test.js",
    "*.test.jsx",
    "*.spec.ts",
    "*.spec.tsx",
    "*.spec.js",
    "*.spec.jsx",
    "**/*.d.ts",
    "**/test/**",
    "**/tests/**",
    "**/__tests__/**",
    "**/coverage/**",
    "**/dist/**",
    "**/build/**",
    "**/node_modules/**",
    "**/.next/**",
    "**/out/**"
  ],
  "ignoreMatches": [
    "@types/*",
    "@typescript-eslint/*",
    "eslint*",
    "prettier*",
    "typescript",
    "ts-node",
    "tsx",
    "vitest",
    "jest",
    "@testing-library/*",
    "webpack*",
    "vite*",
    "@vitejs/*",
    "rollup*",
    "@rollup/*",
    "terser",
    "esbuild",
    "swc*",
    "@swc/*",
    "babel*",
    "@babel/*",
    "postcss*",
    "autoprefixer",
    "tailwindcss",
    "css-loader",
    "style-loader",
    "file-loader",
    "url-loader",
    "html-webpack-plugin",
    "mini-css-extract-plugin",
    "copy-webpack-plugin",
    "clean-webpack-plugin",
    "webpack-bundle-analyzer",
    "source-map-explorer",
    "cross-env",
    "dotenv",
    "dotenv-expand",
    "concurrently",
    "nodemon",
    "pm2",
    "husky",
    "lint-staged",
    "commitizen",
    "cz-conventional-changelog",
    "@commitlint/*",
    "semantic-release",
    "@semantic-release/*",
    "release-it",
    "standard-version"
  ],
  "ignoreDirs": [
    "node_modules",
    "dist",
    "build",
    "coverage",
    ".next",
    "out",
    ".turbo",
    ".vercel",
    ".cache",
    "tmp",
    "temp",
    "prisma/migrations",
    "prisma/dev.db"
  ],
  "ignoreBinPackage": true,
  "skipMissing": true,
  "parsers": {
    "*.ts": "typescript",
    "*.tsx": "typescript",
    "*.js": "es6",
    "*.jsx": "es6",
    "*.json": "json",
    "*.prisma": "prisma"
  },
  "detectors": [
    "requireCallExpression",
    "importDeclaration",
    "exportDeclaration",
    "gruntLoadTaskCallExpression",
    "requireResolveCallExpression",
    "importCallExpression",
    "dynamicImport",
    "typescriptImportEqualsDeclaration",
    "typescriptImportType",
    "typescriptExportAssignment",
    "typescriptModuleReference",
    "commonjsRequireCall",
    "commonjsRequireResolveCall",
    "commonjsDynamicRequire",
    "amdRequireCall",
    "amdDefineCall",
    "umdRequireCall",
    "umdDefineCall",
    "systemjsImportCall",
    "systemjsRegisterCall"
  ],
  "specials": [
    "eslint",
    "babel",
    "typescript",
    "webpack",
    "vite",
    "rollup",
    "jest",
    "vitest",
    "prettier",
    "postcss",
    "tailwindcss",
    "next",
    "nuxt",
    "svelte",
    "vue",
    "react",
    "angular",
    "electron",
    "bin",
    "husky",
    "lint-staged",
    "commitizen",
    "semantic-release",
    "prisma"
  ],
  "package": {
    "ignoreMatches": [
      "express",
      "cors",
      "helmet",
      "express-rate-limit",
      "swagger-jsdoc",
      "swagger-ui-express",
      "express-validator",
      "@aws-sdk/client-s3",
      "@aws-sdk/s3-presigned-post",
      "argon2",
      "jsonwebtoken",
      "bcryptjs",
      "multer",
      "dotenv",
      "dotenv-expand",
      "mysql2",
      "sqlite3",
      "sqlite",
      "p-map",
      "cli-progress",
      "@emotion/is-prop-valid",
      "prisma",
      "@prisma/client",
      "vitest",
      "typescript"
    ]
  },
  "ignorePaths": [
    "dist/**",
    "build/**",
    "coverage/**",
    "prisma/migrations/**",
    "prisma/dev.db",
    "prisma/dev.db-journal",
    "prisma/*.db",
    "prisma/*.db-journal",
    "src/**/*.d.ts",
    "vitest.config.ts",
    "tsconfig.json",
    "tsconfig.app.json",
    "tsconfig.node.json",
    "Dockerfile",
    "Dockerfile.prod"
  ],
  "ignorePackages": [
    "@types/node",
    "vitest",
    "typescript",
    "prisma",
    "@prisma/client"
  ]
}

