  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^11.0.3",
    "mysql2": "^3.6.5",
    "redis": "^4.6.12",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1501.0",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1",
