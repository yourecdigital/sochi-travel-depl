    "@typescript-eslint/parser": "^6.14.0",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "vitest": "^1.0.4",
    "prisma": "^5.7.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
